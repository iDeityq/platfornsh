{
  "chmod": "0755",
  "directories": [
    "bin",
    "resources",
    "src"
  ],
  "files": [
    "config.yaml",
    "constants.php",
    "services.yaml",
    "shell-config.rc",
    "shell-config-bash.rc",
    "vendor/autoload.php",
    "vendor/symfony/console/Resources/bin/hiddeninput.exe",
    "vendor/composer/ca-bundle/res/cacert.pem"
  ],
  "finder": [
    {
      "in": "vendor",
      "name": "*.php*",
      "exclude": [
        "phpunit",
        "Tests",
        "tests"
      ]
    }
  ],
  "main": "bin/platform",
  "output": "platform.phar",
  "stub": "stub.php",
  "compactors": [
      "KevinGH\\Box\\Compactor\\Json",
      "KevinGH\\Box\\Compactor\\Php"
  ]
}
